#include <stdio.h>

int main() {
    int32_t number;
    number = 0;
    int number_scanf;
    scanf("%d\n", &number_scanf);

    int number_i = 0;
    while (number_i < number_scnaf) {
        int32_t number_x;
        scanf("%d\n", &number_x);
        number += number_x;
        number_i += 1;
    }
    printf("sum of numbers is %d\n", number);

    return 0;
}
